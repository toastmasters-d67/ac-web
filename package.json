{
  "name": "ac",
  "version": "1.1.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "serve": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vite test --mode unit",
    "lint": "eslint --ext .js,.vue src --fix"
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "axios": "^1.6.5",
    "primeicons": "^6.0.1",
    "primevue": "^3.46.0",
    "vue": "^3.4.14",
    "vue-i18n": "^9.9.0",
    "vue-router": "^4.2.5",
    "vuex": "^4.0.2",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/test-utils": "^2.4.3",
    "chai": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.20.1",
    "prettier": "^3.2.4",
    "sass": "^1.70.0",
    "vite": "^5.0.11",
    "vite-plugin-env-compatible": "^2.0.1",
    "vite-plugin-html": "3.2.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "no-useless-escape": 0,
      "vue/multi-word-component-names": 0
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
